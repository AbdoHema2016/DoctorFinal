# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Dr. Abdelrhman Arwish Clinic' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Doc
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  
  # Pods for google
  pod 'GoogleSignIn'

  #Pods for database
  pod 'Firebase/Database'

  # Pods SDWebImage
  pod 'SDWebImage'

  # Pods for Facebook
  pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
end